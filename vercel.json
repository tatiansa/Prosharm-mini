{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/.well-known/farcaster.json",
      "destination": "/public/.well-known/farcaster.json"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
